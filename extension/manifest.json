{
    "manifest_version": 2,
    "name": "Phishing Extension",
    "version": "1.0",
    "description": "Prevent phishing attacks through email tracking and URL analysis.",
    "permissions": [
      "activeTab",
      "storage",
      "webRequest",
      "webRequestBlocking",
      "tabs",
      "<all_urls>"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ],
    "web_accessible_resources": [
      "intermediatepage.html",
      "intermediatepage.js"
    ]
  }
  